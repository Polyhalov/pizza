{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "sourceMap": true
  },
  "include": ["src"]
}
